{
  "_version": "1.65.0",
  "sap.app": {
    "id": "project1.request",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.1",
      "toolsId": "b229c214-af63-4df2-b4ea-fd00380d9252"
    },
    "dataSources": {
      "mainService": {
        "uri": "/srv-api/request/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.134.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.component.request.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "groupId": "$auto",
          "synchronizationMode": "None",
          "autoExpandSelect": true
        }
      },
      "Request": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "RequestOrder.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "project1.component.request.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Request_home",
          "pattern": "",
          "target": [
            "Request_home"
          ]
        },
        {
          "name": "Request",
          "pattern": "Request",
          "target": [
            "Request"
          ]
        },
        {
          "name": "CreateOrder",
          "pattern": "Request/CreateOrder/{num}",
          "target": [
            "CreateOrder"
          ]
        },
        {
          "name": "OrderDetail",
          "pattern": "Request/OrderDetail/{num}",
          "target": [
            "OrderDetail"
          ]
        }
      ],
      "targets": {
        "Request_home": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Request_home",
          "viewName": "Request_home"
        },
        "Request": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Request",
          "viewName": "Request"
        },
        "CreateOrder": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "CreateOrder",
          "viewName": "CreateOrder"
        },
        "OrderDetail": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "OrderDetail",
          "viewName": "OrderDetail"
        }
      }
    },
    "rootView": {
      "viewName": "project1.component.request.view.app",
      "type": "XML",
      "async": true,
      "id": "appView"
    }
  }
}